<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="task-center"/>

    <dubbo:monitor protocol="registry"/>

    <!-- 使用multicast广播注册中心暴露发现服务地址 -->
    <dubbo:registry protocol="zookeeper" address="${dubbo.zookeeper.address}"/>
    <dubbo:protocol name="dubbo" port="20296"/>

    <dubbo:consumer check="false"/>

    <!--消费者 -->

    <dubbo:service ref="taskFacde"
                   interface="com.treefinance.saas.taskcenter.facade.service.TaskFacade"
                   version="1.0.0" validation="true"/>

    <dubbo:service ref="taskLogFacade"
                   interface="com.treefinance.saas.taskcenter.facade.service.TaskLogFacade" version="1.0.0"
                   validation="true"/>
    <dubbo:service ref="taskAttributeFacde"
                   interface="com.treefinance.saas.taskcenter.facade.service.TaskAttributeFacade" version="1.0.0"
                   validation="true"/>
    <dubbo:service ref="taskCallBackLogFacade"
                   interface="com.treefinance.saas.taskcenter.facade.service.TaskCallbackLogFacade" version="1.0.0"
                   validation="true"/>

    <dubbo:service ref="taskBuryPointLogFacade"
                   interface="com.treefinance.saas.taskcenter.facade.service.TaskBuryPointLogFacade" version="1.0.0"
                   validation="true"/>



</beans>


